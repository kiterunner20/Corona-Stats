<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="CovidCustomText" parent="Widget.AppCompat.TextView">
        <item name="android:letterSpacing">.09</item>
        <item name="android:textColor">@color/purple</item>
        <item name="fontFamily">@font/poppins_bold</item>
        <item name="android:padding">@dimen/medium_x_text</item>
    </style>

    <style name="labeltext" parent="Widget.AppCompat.TextView">
        <item name="android:letterSpacing">.09</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:padding">@dimen/small</item>
    </style>
    <style name="labeltextv2" parent="Widget.AppCompat.TextView">
        <item name="android:letterSpacing">.09</item>
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:padding">@dimen/small</item>
    </style>

    <style name="displaytext" parent="Widget.AppCompat.TextView">
        <item name="android:letterSpacing">.09</item>
        <item name="android:textColor">@color/amber</item>
        <item name="fontFamily">@font/poppins_medium</item>
        <item name="android:padding">@dimen/small_x</item>
    </style>

    <style name="recoved">Recovered</style>


</resources>